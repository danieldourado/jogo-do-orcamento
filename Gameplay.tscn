script/source = "extends Node2D

var decisions = []
var round_n = 0
var max_rounds = 4
var decisions_per_round = 4

func _ready():
	var weighted_list = get_weighted_list(Global.categories)
	advance_round(weighted_list, round_n, max_rounds, decisions_per_round)
		
func advance_round(list, round_n, max_rounds, decisions_per_round):
	
	round_n = round_n + 1
	if(round_n > max_rounds): return
	
	var index = 0
	var temp_dialog
	for i in range(decisions_per_round):
		index = Global.get_random(list.size())
		temp_dialog = load(\"res://Dialog.tscn\").instance()
		temp_dialog.set_dialog_type(list[index])
		get_parent().get_node(\"CanvasLayer\").add_child(temp_dialog)
		list.remove(index)
	
func get_weighted_list(categories):
	var list = []
	for category in categories:
		for i in range(category.value):
			list.append(category.key)
			list.append(category.key)
	return list
	
"

